	public static void mergeOverlappingIntervals(int[][] arr){
	    Pair[] pairs = new Pair[arr.lenth];
	    for(int i = 0;i<arr.length;i++){
	        pairs[i] = new Pair(arr[i][0],arr[i][1]);// 0 start time , 1 end time 
	    }
	    Arrays.sort(pairs);
	    Stack<Pair>st = new Stack<>();
	    for(int i = 0;i < Pairs.length;i++){
	        if(i == 0){
	            st.push(pairs[i]);
	        }
	        else{
	            Pair top = st.peek();
	            
	            if(pairs[i].st >= top.et){
	                st.push(pairs[i]);
	            }else{
	                top.et = Math.max(top.et,pairs[i].et);
	            }
	        }
	    }
	    /*
	    java app ke integers ke array ko sort kar deti , but objects ke array ko kese sort kare , to app ko batana padega 
	    bhai sort karo ,,,
	    
	            to kisko chota manna padega and kis ko bada manna padega 
	            
	    */
	    Stack<Pair>rs = new Stack<>(); // result stack 
	    while(st.size() > 0){
	        rs.push(st.pop());
	        
	    }
	    while(rs.size() > 0){
	        Pair p = rs.pop();
	        System.out.println(p.st + " " + p.et);
	    }
	}
	public static class Pair implements Comparable<Pair>{ 
	    // ### objects ka chote bade ahesas karna cahte ho , uss object ke upar **interface**** likh dijiye ga ,### Comparable nam ka 
	    int st;// start time
	    int et;
	    Pair(int st,int et){
	        this.st = st;
	        this.et = et;
	    }
	    // array of pairs ko sort then : - Comparable of pair implements
	    
	    // this > other return +ve 
	    // this < other return -ve
	    // this = other return 0
	    public int comparableTo(Pair Other){
	        if(this.st != ohter.st){
	            return this.st - other.st;// jiska start time chota usko array sort hone ke bad pehle rakh lega 
	            
	        }
	        else{
	            return this.et - other.st;
	        }
	    }
	}
}
